{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2020",
        "lib": ["ES2020", "dom"],
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
        "strict": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "types": ["bf6-portal-mod-types"]
    },
    "include": [
        "./**/*.ts"
    ]
}
